# set -x
go test -c -v
for a in BasicIO \
         PingPong \
         RacyRead \
         RacyWrite \
         ReadTimeout \
         WriteTimeout \
         PastTimeout \
         PresentTimeout \
         FutureTimeout \
         CloseTimeout \
         ConcurrentMethods
do
    ./go-libutp.test -test.run "/$a" -test.timeout 1s &> /dev/null
    echo $a $?
done
